generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Producto {
  id        Int      @id @default(autoincrement())
  nombre    String   @db.VarChar(50)
  precio    Float
  categoria String?  @db.VarChar(100)
  creadoEn  DateTime @default(now()) @map("creado_en")

  @@map("productos")
}